{
	"ACVersion": "1.0.0",
	"ACType": "AppManifest",
	"Name": "example.com/ourapp-1.0.0",
	"OS": "linux",
	"Arch": "amd64",
	"Exec": ["/usr/bin/mysql"],
	"Type": "oneshot",
	"User": "100",
	"Group": "300",
	"Environment": {
		"MYSQL_DEBUG": "true"
	},
	"MountPoints": {
		"database": {
			"Path": "/var/lib/mysql",
			"ReadOnly": false
		}
	},
	"Ports": {
		"mysql": {
			"Protocol": "tcp",
			"Port": 3306
		}
	},
	"Isolators": {
		"PrivateNetwork": "true",
		"CpuShares": "20",
		"MemoryLimit": "1G",
		"CapabilityBoundingSet": "CAP_NET_BIND_SERVICE CAP_SYS_ADMIN"
	},
	"Annotations": {
		"Foo": "Bar"
	}
}
